<!-- A successful security domain for H2 database -->
<bean name="SuccessfulSecurityDomain"
      interface="org.jboss.jca.core.spi.security.SubjectFactory"
      class="org.jboss.jca.core.security.DefaultSubjectFactory">
  <property name="SecurityDomain">SuccessfulSecurityDomain</property>
  <property name="UserName">sa</property>
  <property name="Password">sa</property>
</bean>
